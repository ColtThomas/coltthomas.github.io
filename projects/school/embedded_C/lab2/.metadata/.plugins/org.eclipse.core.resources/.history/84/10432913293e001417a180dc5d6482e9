
#include <stdio.h>
#include "supportFiles/leds.h"
#include "supportFiles/display.h"
#include "switches.h"
#define GREEN 0x07E0
#define YELLOW 0xFFE0
#define RED 0xF800
int main(){
	display_init();
	display_fillScreen(DISPLAY_BLACK);  // Blank the screen.
	display_setCursor(0, 0);            // The upper left of the LCD screen.
	display_setTextColor(DISPLAY_RED);  // Make the text red.
	display_setTextSize(2);             // Make the text a little larger.
	display_println("Is it any different?... \n");    // This actually prints the string to the LCD.
	printf("Running switches test\n\r");  // This prints on the console.
	leds_runTest();
	//switches_runTest();
	display_println("Switches test done.");

}
